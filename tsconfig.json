{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2019",
        "lib": [
            "ES2019"
        ],
        "outDir": "out",
        "rootDir": "src",
        "sourceMap": true,
        "strict": true,
        "resolveJsonModule": true,
        "moduleResolution": "Node",
        "skipLibCheck": true,
        "baseUrl": ".",
        "paths": {
            "@interop/*": [
                "interop/src/*"
            ]
        },
        "esModuleInterop": true
    },
    "repository": {},
    "engines": {
        "vscode": "^1.47.0"
    },
    "exclude": [
        "config-gui",
        "interop"
    ],
    "references": [
        {
            "path": "interop"
        }
    ]
}