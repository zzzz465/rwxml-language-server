{
	"name": "rwxml-lang-serv",
	"version": "0.4.5",
	"description": "dev build (experimental)",
	"main": "out/client/extension.js",
	"scripts": {
		"pkgvars": "node ./node_modules/vscode/bin/install",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"test": "jest",
		"lint:fix": "eslint --ext .ts --ignore-path .gitignore . --fix",
		"prepublish": "tsc && dotnet build ./typeinfo_extractor/typeinfo_extractor.sln",
		"webpack": "webpack --mode development",
		"vscode:prepublish": "npm run prepublish && webpack --mode production",
		"webpack-dev": "webpack --mode development --watch"
	},
	"author": "madeline",
	"contributors": [
		"madeline"
	],
	"license": "ISC",
	"categories": [],
	"engines": {
		"vscode": "^1.43.0"
	},
	"repository": {
		"url": "https://github.com/zzzz465/rwxml-language-server"
	},
	"contributes": {
		"colors": [
			{
				"id": "rwxml.decorator.inlayHints.backgroundColor",
				"description": "default background color for inlay-hints (same as rust-inlay-hints)",
				"defaults": {
					"dark": "#61616191",
					"highContrast": "#61616191",
					"light": "#61616191"
				}
			},
			{
				"id": "rwxml.decorator.inlayHints.foregroundColor",
				"description": "default foreground color for inlay-hints (same as rust-inlay-hints)",
				"defaults": {
					"dark": "#a0a0a0f0",
					"highContrast": "#a0a0a0f0",
					"light": "#a0a0a0f0"
				}
			},
			{
				"id": "rwxml.decorator.content.enum.foregroundColor",
				"description": "foreground color for enum variables",
				"defaults": {
					"dark": "symbolIcon.enumeratorForeground",
					"highContrast": "symbolIcon.enumeratorForeground",
					"light": "symbolIcon.enumeratorForeground"
				}
			},
			{
				"id": "rwxml.decorator.content.integer.foregroundColor",
				"description": "foreground color for integer",
				"defaults": {
					"dark": "#b5cea8",
					"highContrast": "#b5cea8",
					"light": "#b5cea8"
				}
			},
			{
				"id": "rwxml.decorator.content.float.foregroundColor",
				"description": "foreground color for float",
				"defaults": {
					"dark": "#c586c0",
					"highContrast": "#c586c0",
					"light": "#c586c0"
				}
			},
			{
				"id": "rwxml.decorator.node.tag.foregroundColor",
				"description": "foreground color for node tag",
				"defaults": {
					"dark": "#9B518B",
					"highContrast": "#9B518B",
					"light": "#9B518B"
				}
			},
			{
				"id": "rwxml.decorator.content.boolean.foregroundColor",
				"description": "foreground color for bool",
				"defaults": {
					"dark": "#569CD6",
					"highContrast": "#569CD6",
					"light": "#569CD6"
				}
			},
			{
				"id": "rwxml.decorator.invalid.node.tag.foregroundColor",
				"description": "foreground color for invalid node tag",
				"defaults": {
					"dark": "#757575",
					"light": "#757575",
					"highContrast": "#757575"
				}
			}
		]
	},
	"publisher": "madeline",
	"jest": {
		"transform": {
			"^.+\\.ts$": "ts-jest"
		},
		"testRegex": "\\.test\\.ts$",
		"moduleFileExtensions": [
			"js",
			"ts"
		],
		"preset": "ts-jest",
		"testMatch": null
	},
	"activationEvents": [
		"onLanguage:xml"
	],
	"devDependencies": {
		"@babel/core": "^7.11.1",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-typescript": "^7.10.4",
		"@types/jest": "^26.0.9",
		"@types/lodash": "^4.14.159",
		"@types/node": "^12.12.0",
		"@typescript-eslint/eslint-plugin": "^3.9.0",
		"@typescript-eslint/parser": "^2.34.0",
		"babel-jest": "^26.3.0",
		"eslint": "^6.4.0",
		"eslint-config-typescript": "^3.0.0",
		"eslint-plugin-only-warn": "^1.0.2",
		"jest": "^26.3.0",
		"ts-loader": "^8.0.3",
		"typescript": "^3.9.4",
		"vscode": "^1.1.37",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"@types/glob": "^7.1.3",
		"glob": "^7.1.6",
		"linq-es2015": "^2.5.1",
		"lodash": "^4.17.19",
		"node-watch": "^0.6.4",
		"ts-jest": "^26.1.4",
		"tsconfig-paths": "^3.9.0",
		"vscode-languageclient": "^6.1.3",
		"vscode-languageserver": "^6.1.1",
		"vscode-languageserver-textdocument": "^1.0.1",
		"vscode-languageserver-types": "^3.15.1",
		"vscode-nls": "^4.1.2",
		"vscode-uri": "^2.1.2"
	}
}