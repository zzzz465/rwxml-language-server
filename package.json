{
  "name": "rwxml-language-server",
  "version": "1.0.0",
  "repository": "https://github.com/zzzz465/rwxml-language-server.git",
  "author": "madeline <zzzz465@naver.com>",
  "license": "",
  "private": true,
  "workspaces": [
    "analyzer",
    "language-server",
    "vsc-extension"
  ],
  "scripts": {
    "install/analyzer": "npm --prefix analyzer install",
    "install/extractor": "npm --prefix extractor install",
    "install/language-server": "npm --prefix language-server install",
    "install/vsc-extension": "npm --prefix vsc-extension install",
    "build/analyzer": "npm --prefix analyzer run build",
    "build/extractor": "npm --prefix extractor run build",
    "build/language-server": "npm --prefix language-server run build",
    "build": "npm run build/analyzer && npm run build/language-server",
    "clear": "rimraf ./*/dist",
    "purge": "rimraf ./*/node_modules",
    "reinstall": "npm run clear && npm run purge && npm install",
    "postinstall": "npm run install/analyzer && npm run install/language-server && npm run install/vsc-extension && npm run build/analyzer",
    "watch": "npm run build/analyzer -- --watch"
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^9.0.1",
    "webpack": "^5.72.1"
  }
}
